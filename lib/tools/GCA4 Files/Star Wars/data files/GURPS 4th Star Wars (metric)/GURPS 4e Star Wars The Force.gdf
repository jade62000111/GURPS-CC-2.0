[ATTRIBUTES]
******************************************************************************************************************
*Basic Attributes
Dark Side Points, basevalue(0), display(No), minscore(0), maxscore(10000), step(1), Hide(yes)

Force Sight, basevalue(ST:Perception), maxscore(1000000), minscore(0)

* Force Sensitivity Prerequisite Work-around Stats
Force Sensitivity, basevalue(0), display(no), minscore(0), maxscore(5), hide(yes)

*Force Sensitivity Aspect Work-around Stats
Force Sensitivity Dark Aspect, basevalue(), display(no), minscore(0), maxscore(5), hide(yes),
gives(_
	=-me::score to ("GR:Force Abilities"),
	=+me:score to ("GR:Dark Side Force Abilities")_
	)
Force Sensitivity Light Aspect, basevalue(), display(no), minscore(0), maxscore(5), hide(yes),
gives(_
	=-me::score to ("GR:Force Abilities"),
	=+me:score to ("GR:Light Side Force Abilities")_
	)
Force Sensitivity Neutral Aspect, basevalue(), display(no), minscore(0), maxscore(5), hide(yes),
gives(_
	=-me::score to ("GR:Force Abilities"),
	=+me:score to ("GR:Neutral Side Force Abilities")_
	)

*Force Crystals Work-around Stats
Force Sensitivity Heal Self, basevalue(), display(no), minscore(0), maxscore(5), hide(yes),
gives(_
	=+me::score to ("SP:Heal Self")_
	)

*Unique Force Abilities Work-around Stats
Unique Force Abilities, basevalue(0), display(no), minscore(0), maxscore(1), hide(yes)

